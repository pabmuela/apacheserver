# <%# Virtual hosts template -%>
<VirtualHost *:<%= $port %>>
    ServerName <%= $subdomain %>.<%= $facts[fqdn] %>
    ServerAlias <%= $subdomain %>
<% if $admin =~ String[1] { %>
    ServerAdmin <%= $admin %>
<% } %>
    DocumentRoot <%= $docroot %>
</VistualHost>
